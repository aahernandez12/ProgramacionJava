package ClasesVariables;

public class Ejercicio1 {

	public static void main(String[] args) {
		String nombre = "Alfredo Hernández López";
		double x = 5.23;
		final double valorDolar = 20.03;
		float valorPi = 3.1415927F;
		double euler = Math.E;
		
		System.out.println("Mi nombre es: "+nombre);
		System.out.println("Décimales: " +x);
		System.out.println("El valor de Dolar en México es: " +valorDolar);
		System.out.println("Primero 8 decimales de Pi: " +valorPi);
		System.out.println("Primero 18 décimales de Euler: " +euler);
		
	
	}
}